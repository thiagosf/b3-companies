  "license": "MIT",
  "private": false,
  "dependencies": {
    "axios": "^0.19.0",
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "cheerio": "^1.0.0-rc.3",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.3",
    "graceful-fs": "^4.2.3",
    "lodash": "^4.17.19",
    "mocha": "^6.2.2",
    "moment": "^2.24.0",
    "mysql2": "^2.0.1",
    "node-schedule": "^1.3.2",
    "node-telegram-bot-api": "^0.40.0",
    "nodemon": "^1.19.4",
    "puppeteer": "^2.0.0",
    "sequelize": "^5.21.2",
    "sequelize-cli": "^5.5.1",
    "slugify": "^1.3.5",
    "tor-axios": "^1.0.7"
  },
  "scripts": {
    "test": "mocha test/*_test.js",
    "prestart": "yarn run migrate",
    "start": "env $(cat .env) nodemon --watch src src/server.js",
    "build": "babel --copy-files --out-dir dist src",
    "seq": "sequelize --config sequelize_settings.js",
    "migrate": "npm run seq db:migrate",
    "seed": "npm run seq db:seed:all",
    "rollback": "npm run seq db:migrate:undo"
  }
